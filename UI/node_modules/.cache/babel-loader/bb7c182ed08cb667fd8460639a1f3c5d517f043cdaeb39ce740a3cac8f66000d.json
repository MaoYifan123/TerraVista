{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport AMapLoader from '@amap/amap-jsapi-loader';\nexport default {\n  name: 'AMapDemo',\n  data() {\n    return {\n      map: null,\n      markers: []\n    };\n  },\n  mounted() {\n    this.initMap();\n  },\n  methods: {\n    initMap() {\n      AMapLoader.load({\n        key: '47d6f6593f6cf161673081c8ae38526b',\n        // 替换为你的key\n        version: '2.0',\n        // 指定要加载的 JSAPI 的版本\n        plugins: ['AMap.ToolBar', 'AMap.Scale', 'AMap.OverView', 'AMap.Geocoder'] // 需要使用的的插件列表\n      }).then(AMap => {\n        this.map = new AMap.Map('map-container', {\n          viewMode: '2D',\n          // 默认使用 2D 模式\n          zoom: 11,\n          // 初始化地图级别\n          center: [116.397428, 39.90923] // 初始化地图中心点位置（北京天安门）\n        });\n\n        // 添加控件\n        this.map.addControl(new AMap.ToolBar());\n        this.map.addControl(new AMap.Scale());\n        this.map.addControl(new AMap.OverView());\n\n        // 添加标记点\n        this.addMarker(AMap);\n\n        // 添加点击事件\n        this.map.on('click', e => {\n          console.log('点击位置经纬度:', e.lnglat);\n        });\n      }).catch(e => {\n        console.error('地图加载失败:', e);\n      });\n    },\n    addMarker(AMap) {\n      // 创建标记点\n      const marker = new AMap.Marker({\n        position: [116.397428, 39.90923],\n        // 位置\n        title: '北京天安门'\n      });\n\n      // 创建信息窗口\n      const infoWindow = new AMap.InfoWindow({\n        content: '<div style=\"padding:5px;\">这里是北京天安门</div>',\n        offset: new AMap.Pixel(0, -30)\n      });\n\n      // 标记点点击事件\n      marker.on('click', () => {\n        infoWindow.open(this.map, marker.getPosition());\n      });\n\n      // 将标记添加到地图\n      this.map.add(marker);\n      this.markers.push(marker);\n    }\n  },\n  beforeDestroy() {\n    // 销毁地图\n    if (this.map) {\n      this.map.destroy();\n    }\n  }\n};","map":{"version":3,"names":["AMapLoader","name","data","map","markers","mounted","initMap","methods","load","key","version","plugins","then","AMap","Map","viewMode","zoom","center","addControl","ToolBar","Scale","OverView","addMarker","on","e","console","log","lnglat","catch","error","marker","Marker","position","title","infoWindow","InfoWindow","content","offset","Pixel","open","getPosition","add","push","beforeDestroy","destroy"],"sources":["C:\\Users\\35026\\IdeaProjects\\SpringVue\\UI\\src\\views\\Map.vue"],"sourcesContent":["<template>\r\n  地图页面\r\n  <div class=\"map-container\">\r\n    <div id=\"map-container\" style=\"width: 100%; height: 500px\"></div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport AMapLoader from '@amap/amap-jsapi-loader'\r\n\r\nexport default {\r\n  name: 'AMapDemo',\r\n  data() {\r\n    return {\r\n      map: null,\r\n      markers: []\r\n    }\r\n  },\r\n  mounted() {\r\n    this.initMap()\r\n  },\r\n  methods: {\r\n    initMap() {\r\n      AMapLoader.load({\r\n        key: '47d6f6593f6cf161673081c8ae38526b', // 替换为你的key\r\n        version: '2.0', // 指定要加载的 JSAPI 的版本\r\n        plugins: ['AMap.ToolBar', 'AMap.Scale', 'AMap.OverView', 'AMap.Geocoder'] // 需要使用的的插件列表\r\n      })\r\n          .then((AMap) => {\r\n            this.map = new AMap.Map('map-container', {\r\n              viewMode: '2D', // 默认使用 2D 模式\r\n              zoom: 11, // 初始化地图级别\r\n              center: [116.397428, 39.90923] // 初始化地图中心点位置（北京天安门）\r\n            })\r\n\r\n            // 添加控件\r\n            this.map.addControl(new AMap.ToolBar())\r\n            this.map.addControl(new AMap.Scale())\r\n            this.map.addControl(new AMap.OverView())\r\n\r\n            // 添加标记点\r\n            this.addMarker(AMap)\r\n\r\n            // 添加点击事件\r\n            this.map.on('click', (e) => {\r\n              console.log('点击位置经纬度:', e.lnglat)\r\n            })\r\n          })\r\n          .catch(e => {\r\n            console.error('地图加载失败:', e)\r\n          })\r\n    },\r\n\r\n    addMarker(AMap) {\r\n      // 创建标记点\r\n      const marker = new AMap.Marker({\r\n        position: [116.397428, 39.90923], // 位置\r\n        title: '北京天安门'\r\n      })\r\n\r\n      // 创建信息窗口\r\n      const infoWindow = new AMap.InfoWindow({\r\n        content: '<div style=\"padding:5px;\">这里是北京天安门</div>',\r\n        offset: new AMap.Pixel(0, -30)\r\n      })\r\n\r\n      // 标记点点击事件\r\n      marker.on('click', () => {\r\n        infoWindow.open(this.map, marker.getPosition())\r\n      })\r\n\r\n      // 将标记添加到地图\r\n      this.map.add(marker)\r\n      this.markers.push(marker)\r\n    }\r\n  },\r\n\r\n  beforeDestroy() {\r\n    // 销毁地图\r\n    if (this.map) {\r\n      this.map.destroy()\r\n    }\r\n  }\r\n}\r\n</script>"],"mappings":";;;AAQA,OAAOA,UAAS,MAAO,yBAAwB;AAE/C,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,GAAG,EAAE,IAAI;MACTC,OAAO,EAAE;IACX;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,OAAO,CAAC;EACf,CAAC;EACDC,OAAO,EAAE;IACPD,OAAOA,CAAA,EAAG;MACRN,UAAU,CAACQ,IAAI,CAAC;QACdC,GAAG,EAAE,kCAAkC;QAAE;QACzCC,OAAO,EAAE,KAAK;QAAE;QAChBC,OAAO,EAAE,CAAC,cAAc,EAAE,YAAY,EAAE,eAAe,EAAE,eAAe,EAAE;MAC5E,CAAC,EACIC,IAAI,CAAEC,IAAI,IAAK;QACd,IAAI,CAACV,GAAE,GAAI,IAAIU,IAAI,CAACC,GAAG,CAAC,eAAe,EAAE;UACvCC,QAAQ,EAAE,IAAI;UAAE;UAChBC,IAAI,EAAE,EAAE;UAAE;UACVC,MAAM,EAAE,CAAC,UAAU,EAAE,QAAQ,EAAE;QACjC,CAAC;;QAED;QACA,IAAI,CAACd,GAAG,CAACe,UAAU,CAAC,IAAIL,IAAI,CAACM,OAAO,CAAC,CAAC;QACtC,IAAI,CAAChB,GAAG,CAACe,UAAU,CAAC,IAAIL,IAAI,CAACO,KAAK,CAAC,CAAC;QACpC,IAAI,CAACjB,GAAG,CAACe,UAAU,CAAC,IAAIL,IAAI,CAACQ,QAAQ,CAAC,CAAC;;QAEvC;QACA,IAAI,CAACC,SAAS,CAACT,IAAI;;QAEnB;QACA,IAAI,CAACV,GAAG,CAACoB,EAAE,CAAC,OAAO,EAAGC,CAAC,IAAK;UAC1BC,OAAO,CAACC,GAAG,CAAC,UAAU,EAAEF,CAAC,CAACG,MAAM;QAClC,CAAC;MACH,CAAC,EACAC,KAAK,CAACJ,CAAA,IAAK;QACVC,OAAO,CAACI,KAAK,CAAC,SAAS,EAAEL,CAAC;MAC5B,CAAC;IACP,CAAC;IAEDF,SAASA,CAACT,IAAI,EAAE;MACd;MACA,MAAMiB,MAAK,GAAI,IAAIjB,IAAI,CAACkB,MAAM,CAAC;QAC7BC,QAAQ,EAAE,CAAC,UAAU,EAAE,QAAQ,CAAC;QAAE;QAClCC,KAAK,EAAE;MACT,CAAC;;MAED;MACA,MAA