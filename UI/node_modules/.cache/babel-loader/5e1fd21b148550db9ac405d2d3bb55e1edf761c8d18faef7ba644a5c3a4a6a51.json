{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref, reactive } from 'vue';\nimport { useRouter } from 'vue-router';\nimport { login } from '@/api/auth';\nexport default {\n  name: 'Login',\n  setup() {\n    const router = useRouter();\n    const loginForm = reactive({\n      username: '',\n      password: ''\n    });\n    const loading = ref(false);\n    const showPassword = ref(false);\n    const errors = reactive({\n      username: '',\n      password: ''\n    });\n    const validateForm = () => {\n      let isValid = true;\n      errors.username = '';\n      errors.password = '';\n      if (!loginForm.username) {\n        errors.username = '请输入用户名';\n        isValid = false;\n      } else if (loginForm.username.length < 3) {\n        errors.username = '用户名至少需要3个字符';\n        isValid = false;\n      }\n      if (!loginForm.password) {\n        errors.password = '请输入密码';\n        isValid = false;\n      } else if (loginForm.password.length < 6) {\n        errors.password = '密码至少需要6个字符';\n        isValid = false;\n      }\n      return isValid;\n    };\n    const handleLogin = async () => {\n      if (!validateForm()) return;\n      try {\n        loading.value = true;\n        await login(loginForm.username, loginForm.password);\n        router.push('/');\n      } catch (error) {\n        console.error('登录失败:', error);\n      } finally {\n        loading.value = false;\n      }\n    };\n    return {\n      loginForm,\n      loading,\n      showPassword,\n      errors,\n      handleLogin\n    };\n  }\n};","map":{"version":3,"names":["ref","reactive","useRouter","login","name","setup","router","loginForm","username","password","loading","showPassword","errors","validateForm","isValid","length","handleLogin","value","push","error","console"],"sources":["C:\\Users\\35026\\IdeaProjects\\SpringVue\\UI\\src\\views\\Login.vue"],"sourcesContent":["<template>\r\n  <div class=\"login-container\">\r\n    <div class=\"login-card\">\r\n      <h2>登录</h2>\r\n      <form @submit.prevent=\"handleLogin\">\r\n        <div class=\"form-item\">\r\n          <div class=\"input-wrapper\">\r\n            <i class=\"el-icon-user\"></i>\r\n            <input\r\n              type=\"text\"\r\n              v-model=\"loginForm.username\"\r\n              placeholder=\"用户名\"\r\n              maxlength=\"20\"\r\n              class=\"input-field\"\r\n            />\r\n          </div>\r\n          <span class=\"error-message\" v-if=\"errors.username\">{{ errors.username }}</span>\r\n        </div>\r\n        <div class=\"form-item\">\r\n          <div class=\"input-wrapper\">\r\n            <i class=\"el-icon-lock\"></i>\r\n            <input\r\n              :type=\"showPassword ? 'text' : 'password'\"\r\n              v-model=\"loginForm.password\"\r\n              placeholder=\"密码\"\r\n              maxlength=\"20\"\r\n              class=\"input-field\"\r\n            />\r\n            <i \r\n              :class=\"showPassword ? 'el-icon-view' : 'el-icon-hide'\"\r\n              @click=\"showPassword = !showPassword\"\r\n              class=\"password-toggle\"\r\n            ></i>\r\n          </div>\r\n          <span class=\"error-message\" v-if=\"errors.password\">{{ errors.password }}</span>\r\n        </div>\r\n        <div class=\"form-item\">\r\n          <button \r\n            type=\"submit\" \r\n            class=\"login-button\"\r\n            :disabled=\"loading\"\r\n          >\r\n            {{ loading ? '登录中...' : '登录' }}\r\n          </button>\r\n        </div>\r\n        <div class=\"form-item\">\r\n          <a href=\"#\" @click.prevent=\"$router.push('/register')\" class=\"register-link\">\r\n            还没有账号？立即注册\r\n          </a>\r\n        </div>\r\n      </form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, reactive } from 'vue'\r\nimport { useRouter } from 'vue-router'\r\nimport { login } from '@/api/auth'\r\n\r\nexport default {\r\n  name: 'Login',\r\n  setup() {\r\n    const router = useRouter()\r\n    const loginForm = reactive({\r\n      username: '',\r\n      password: ''\r\n    })\r\n    const loading = ref(false)\r\n    const showPassword = ref(false)\r\n    const errors = reactive({\r\n      username: '',\r\n      password: ''\r\n    })\r\n\r\n    const validateForm = () => {\r\n      let isValid = true\r\n      errors.username = ''\r\n      errors.password = ''\r\n\r\n      if (!loginForm.username) {\r\n        errors.username = '请输入用户名'\r\n        isValid = false\r\n      } else if (loginForm.username.length < 3) {\r\n        errors.username = '用户名至少需要3个字符'\r\n        isValid = false\r\n      }\r\n\r\n      if (!loginForm.password) {\r\n        errors.password = '请输入密码'\r\n        isValid = false\r\n      } else if (loginForm.password.length < 6) {\r\n        errors.password = '密码至少需要6个字符'\r\n        isValid = false\r\n      }\r\n\r\n      return isValid\r\n    }\r\n\r\n    const handleLogin = async () => {\r\n      if (!validateForm()) return\r\n\r\n      try {\r\n        loading.value = true\r\n        await login(loginForm.username, loginForm.password)\r\n        router.push('/')\r\n      } catch (error) {\r\n        console.error('登录失败:', error)\r\n      } finally {\r\n        loading.value = false\r\n      }\r\n    }\r\n\r\n    return {\r\n      loginForm,\r\n      loading,\r\n      showPassword,\r\n      errors,\r\n      handleLogin\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.login-container {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  height: 100vh;\r\n  background-color: #f5f7fa;\r\n}\r\n\r\n.login-card {\r\n  width: 400px;\r\n  padding: 30px;\r\n  background: white;\r\n  border-radius: 8px;\r\n  box-shadow: 0 2px 12px 0 rgba(0, 0, 0, 0.1);\r\n}\r\n\r\nh2 {\r\n  text-align: center;\r\n  margin-bottom: 30px;\r\n  color: #409EFF;\r\n}\r\n\r\n.form-item {\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.input-wrapper {\r\n  position: relative;\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n\r\n.input-field {\r\n  width: 100%;\r\n  padding: 12px 40px;\r\n  border: 1px solid #dcdfe6;\r\n  border-radius: 4px;\r\n  font-size: 14px;\r\n  transition: border-color 0.2s;\r\n}\r\n\r\n.input-field:focus {\r\n  outline: none;\r\n  border-color: #409EFF;\r\n}\r\n\r\n.input-wrapper i {\r\n  position: absolute;\r\n  left: 12px;\r\n  color: #909399;\r\n}\r\n\r\n.password-toggle {\r\n  left: auto !important;\r\n  right: 12px;\r\n  cursor: pointer;\r\n}\r\n\r\n.error-message {\r\n  color: #f56c6c;\r\n  font-size: 12px;\r\n  margin-top: 5px;\r\n  display: block;\r\n}\r\n\r\n.login-button {\r\n  width: 100%;\r\n  padding: 12px;\r\n  background-color: #409EFF;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 4px;\r\n  font-size: 14px;\r\n  cursor: pointer;\r\n  transition: background-color 0.2s;\r\n}\r\n\r\n.login-button:hover {\r\n  background-color: #66b1ff;\r\n}\r\n\r\n.login-button:disabled {\r\n  background-color: #a0cfff;\r\n  cursor: not-allowed;\r\n}\r\n\r\n.register-link {\r\n  color: #409EFF;\r\n  text-decoration: none;\r\n  font-size: 14px;\r\n}\r\n\r\n.register-link:hover {\r\n  color: #66b1ff;\r\n}\r\n</style> "],"mappings":";AAwDA,SAASA,GAAG,EAAEC,QAAO,QAAS,KAAI;AAClC,SAASC,SAAQ,QAAS,YAAW;AACrC,SAASC,KAAI,QAAS,YAAW;AAEjC,eAAe;EACbC,IAAI,EAAE,OAAO;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,MAAK,GAAIJ,SAAS,CAAC;IACzB,MAAMK,SAAQ,GAAIN,QAAQ,CAAC;MACzBO,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE;IACZ,CAAC;IACD,MAAMC,OAAM,GAAIV,GAAG,CAAC,KAAK;IACzB,MAAMW,YAAW,GAAIX,GAAG,CAAC,KAAK;IAC9B,MAAMY,MAAK,GAAIX,QAAQ,CAAC;MACtBO,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE;IACZ,CAAC;IAED,MAAMI,YAAW,GAAIA,CAAA,KAAM;MACzB,IAAIC,OAAM,GAAI,IAAG;MACjBF,MAAM,CAACJ,QAAO,GAAI,EAAC;MACnBI,MAAM,CAACH,QAAO,GAAI,EAAC;MAEnB,IAAI,CAACF,SAAS,CAACC,QAAQ,EAAE;QACvBI,MAAM,CAACJ,QAAO,GAAI,QAAO;QACzBM,OAAM,GAAI,KAAI;MAChB,OAAO,IAAIP,SAAS,CAACC,QAAQ,CAACO,MAAK,GAAI,CAAC,EAAE;QACxCH,MAAM,CAACJ,QAAO,GAAI,aAAY;QAC9BM,OAAM,GAAI,KAAI;MAChB;MAEA,IAAI,CAACP,SAAS,CAACE,QAAQ,EAAE;QACvBG,MAAM,CAACH,QAAO,GAA